
@{
    ViewBag.Title = "GetFollower";
}

<h2>GetFollower</h2>

<table class="table">
    <thead>
        <tr>
            <td>
                Name
            </td>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in ViewBag.Follower)
        {
            <tr>
                <td>
                    @user.Name
                </td>
                <td>
                    @Html.ActionLink("details", "UserDetails", "User", new { id = user.Id}, null)
                </td>
            </tr>
        }


    </tbody>
</table>
